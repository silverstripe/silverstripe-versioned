{
    "name": "silverstripe/versioned",
    "description": "SilverStripe Versioned component",
    "type": "silverstripe-vendormodule",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "versioned"
    ],
    "authors": [
        {
            "name": "SilverStripe",
            "homepage": "http://silverstripe.com"
        },
        {
            "name": "The SilverStripe Community",
            "homepage": "http://silverstripe.org"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.7",
        "silverstripe/vendor-plugin": "^1",
        "symfony/cache": "^3.4"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "silverstripe/graphql": "4.x-dev",
        "silverstripe/admin": "^1",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Versioned\\": "src/",
            "SilverStripe\\Versioned\\Tests\\": "tests/php/"
        },
        "classmap": ["src/GraphQL/_legacy"]
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/php/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
